/*2. Звездочки (2)
*******
*-----*
*-----*
*******
*/

#include <stdio.h>
#include <string.h>

int main() {
    for (int i = 1; i <= 4; i++) {
        for (int j = 1; j <= 7; j++) {
            if ((j == 1 || j == 7) || (i == 1 || i == 4)){
                printf("*");
            }
            else {
                printf("-");
            } 
        }
    printf("\n");
    }
    return 0;
}
