/*6. Вводятся числа. Окончание ввода число 0. Найти сумму четных
*/

#include <stdio.h>
#define SIZE 100

int main() {
    int n, sum = 0;
    while (1) {
        scanf("%d", &n);
        sum += n;
        if (n == 0) {
            break;
            }
        }
   
    printf("%d", sum);
    
    return 0;
}
